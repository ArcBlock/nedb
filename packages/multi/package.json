{
  "name": "@nedb/multi",
  "author": "Vladimir Angelov, wangshijun",
  "files": [
    "lib",
    "example",
    "index.js",
    "server.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "version": "1.3.4",
  "description": "A proxy for nedb which allows for multi-process access",
  "scripts": {
    "precommit": "npm run test",
    "prepush": "npm run test",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "tape test/unit/**.js",
    "start-server": "NEDB_MULTI_PORT=5151 node server.js",
    "test-server": "NEDB_MULTI_ITERATIONS=10 NEDB_MULTI_PORT=5151 node test/integration/test.js",
    "test:integration": "rm test.data && start-server-and-test start-server tcp:5151 test-server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ArcBlock/nedb"
  },
  "keywords": [
    "nedb"
  ],
  "license": "MIT",
  "dependencies": {
    "@nedb/core": "1.3.4",
    "axon": "^2.0.3",
    "errio": "^1.2.2"
  },
  "devDependencies": {
    "faucet": "0.0.1",
    "proxyquire": "^1.8.0",
    "start-server-and-test": "^1.14.0",
    "tape": "^4.16.0"
  }
}
